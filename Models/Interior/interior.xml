<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>tour</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>intvitres</object-name>
  </animation>

  <!-- Pilote -->
  <model>
    <path>Aircraft/DO-28/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -3.658 </x-m>
      <y-m> -0.401 </y-m>
      <z-m> -0.547 </z-m>
      <roll-deg>    0.0 </roll-deg>
      <pitch-deg>   0.0 </pitch-deg>
      <heading-deg> 0.0 </heading-deg>
    </offsets>
  </model>

  <!-- Planche de bord -->
  <model>
    <path>Aircraft/DO-28/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -4.181 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.419 </z-m>
    </offsets>
  </model>

</PropertyList>

